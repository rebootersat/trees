package bst;

public class BinarySearchTree<T> {

	private Node root;
	
	public void insert(T ele) {
		if( root == null)
			root = new Node(ele);
		else
			root = insert(root, ele);
	}

	private Node insert(Node root, T ele) {
		if(root == null)
			return new Node(ele);
		
		
		return null;
	}


	private class Node<T>{
		
		private Node left;
		private Node right;
		private T ele;
		

		public Node(T ele) {
			this.ele = ele;
		}
		
		public Node(Node left, Node right, T ele) {
			super();
			this.left = left;
			this.right = right;
			this.ele = ele;
		}
		
		
		
	}
	
}
